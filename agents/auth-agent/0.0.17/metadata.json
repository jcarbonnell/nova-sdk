{
  "name": "auth-agent",
  "version": "0.0.17",
  "description": "Handles group registration, add/revoke members within the NOVA secure file sharing system.",
  "category": "agent",
  "tags": ["auth", "group", "member", "nova"],
  "details": {
    "agent": {
      "defaults": {
        "model": "llama-v3p1-70b-instruct",
        "model_provider": "fireworks",
        "model_temperature": 1.0,
        "model_max_tokens": 16384
      },
      "env_vars": {
        "CONTRACT_ID": "nova-sdk-2.testnet",
        "SIGNER_ACCOUNT_ID": "nova-sdk-2.testnet",
        "NEAR_PRIVATE_KEY": ""
      }
    }
  },
  "show_entry": true
}