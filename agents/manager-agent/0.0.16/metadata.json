{
  "name": "manager-agent",
  "version": "0.0.16",
  "description": "Routes commands within the NOVA secure file sharing system.",
  "category": "agent",
  "tags": ["manager", "routing", "file sharing", "NOVA"],
  "details": {
    "agent": {
      "defaults": {
        "model": "llama-v3p1-70b-instruct",
        "model_provider": "fireworks",
        "model_temperature": 1,
        "model_max_tokens": 16384
      },
      "env_vars": {
        "CONTRACT_ID": "nova-sdk-2.testnet",
        "NEAR_PRIVATE_KEY": ""
      }
    }
  },
  "show_entry": true
}