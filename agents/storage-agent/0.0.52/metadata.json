{
  "name": "storage-agent",
  "version": "0.0.52",
  "description": "Handles file upload/list/retrieve in NOVA (encrypt/IPFS/record).",
  "category": "agent",
  "tags": ["storage", "ipfs", "file", "nova"],
  "details": {
    "agent": {
      "defaults": {
        "model": "llama-v3p1-70b-instruct",
        "model_provider": "fireworks",
        "model_temperature": 1.0,
        "model_max_tokens": 16384
      },
      "env_vars": {
        "CONTRACT_ID": "nova-sdk-2.testnet",
        "IPFS_API_KEY": "",
        "IPFS_API_SECRET": "",
        "NEAR_PRIVATE_KEY": "",
        "PINATA_GATEWAY": "https://green-main-mockingbird-133.mypinata.cloud/ipfs",
        "MCP_URL": "https://nova-mcp.fastmcp.app/mcp"
      }
    }
  },
  "show_entry": true
}